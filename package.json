{
  "name": "turbo-repo-starter-kit",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "find . -type d \\( -name \"node_modules\" -o -name \"dist\" -o -name \".turbo\" \\) -exec rm -rf {} +"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
